encoded_message = [
    73, 39, 109, 32, 103, 111, 105, 110, 103, 32, 116, 111, 32, 112, 114, 111, 112, 111, 115, 101, 32,
    116, 111, 32, 75, 97, 121, 108, 97, 44, 32, 99, 97, 110, 32, 121, 111, 117, 32, 104, 101, 108, 112, 63
]

decoded_message = ''.join(chr(char) for char in encoded_message)
print(decoded_message)

encoded_resp1 = [97, 104, 104, 104, 32, 110, 105, 99, 101, 32, 73, 32, 99, 97, 110, 32, 114, 101, 97, 100, 32, 104, 97, 104, 97]

decoded_resp1 = ''.join(chr(char) for char in encoded_resp1)
print(decoded_resp1)
